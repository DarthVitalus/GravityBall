{
  "extends": "tslint:recommended",
  "rules": {
		"no-debugger": false,
		"no-empty": false,
		"no-trailing-whitespace": false,
		"indent": {
			"options": "tabs"
		},
		"quotemark": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
